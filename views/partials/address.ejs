<address class="sparkeats-address">
  <p><%= place.address %></p>
  <% if (place.phone) { %>
    <p><a class="sparkeats-address__phone" href="tel:<%= place.phone %>"><%= place.phone %></a></p>
  <% } %>
</address>
<% if (place.placeURL) { %>
  <div class="sparkeats-address__url">
    <a class="sparkeats-address__url-link" href="<%= place.placeUrl %>">Visit Site</a>
  </div>
<% } %>
