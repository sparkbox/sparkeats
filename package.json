{
  "name": "sparkeats-v-2",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "@sailshq/connect-redis": "^3.2.1",
    "@sailshq/lodash": "^3.10.3",
    "@sailshq/socket.io-redis": "^5.2.0",
    "async": "2.0.1",
    "autoprefixer": "^8.6.2",
    "dotenv": "5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "express": "4.16.3",
    "grunt": "1.0.1",
    "grunt-sass": "^2.1.0",
    "js-yaml": "^3.12.0",
    "knex": "^0.20.8",
    "node-sass": "^4.9.0",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "postcss-cli": "^5.0.1",
    "prettier": "^1.13.4",
    "ramda": "^0.26.1",
    "sails": "^1.0.2",
    "sails-hook-grunt": "^3.0.2",
    "sails-hook-orm": "^2.0.0-16",
    "sails-hook-sockets": "^1.4.0",
    "sails-mysql": "^1.0.0",
    "stream-to-blob": "^1.0.1",
    "tsscmp": "1.0.5"
  },
  "devDependencies": {
    "@sailshq/eslint": "^4.19.3",
    "browser-sync": "^2.24.4",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0"
  },
  "scripts": {
    "lift": "sails lift --drop",
    "dev": "concurrently \"npm run sass:watch\" \"npm run server\"",
    "start": "npm run build && sails lift",
    "build": "npm run sass && npm run postcss",
    "sass": "node-sass assets/styles/main.scss .tmp/styles/main.css",
    "sass:watch": "node-sass --watch assets/styles/main.scss .tmp/styles/main.css",
    "postcss": "postcss --use autoprefixer -c tasks/postcss.config.json .tmp/styles/main.css -d .tmp/styles/",
    "server": "nodemon app.js & browser-sync start --proxy http://localhost:1337 --files \".tmp/styles/\" \"views/\" --reload-delay 1100",
    "test:unit": "node ./node_modules/mocha/bin/mocha test/unit/*.test.js",
    "test:integration": "node ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/ test/integration/controllers/**/*.test.js",
    "lint": "eslint . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/travisdagreat/sparkeats-v-2.git"
  },
  "author": "travisdagreat et al",
  "license": "",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  }
}
